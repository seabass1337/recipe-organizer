<h1>New Pantry Item</h1>
<%= form_for :pantry_item, url: pantry_items_path do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %> 
<%= link_to 'Back', pantry_items_path %>
