<h1>Create a new shopping list</h1>
<%= form_for @shopping_list, url: shopping_lists_path do |f| %>
  <p>
    <% f.label :name %>
    <% f.text_field :name %>
  </p>
  <p>
    <% f.label :amount %>
    <% f.text_field :amount %>
  </p>
  <p>
    <% f.submit %>
  </p>
<% end %>
<%= link_to "Back", shopping_lists_path %>
